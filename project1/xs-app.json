{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "authenticationType": "xsuaa",
      "csrfProtection": false,
      "source": "^/sap/opu/odata/sap/ZEWM_PBO_SRV/",
      "destination": "ER9_CLONING"
    },
    {
      "authenticationType": "xsuaa",
      "csrfProtection": false,
      "source": "^/sap/opu/odata/sap/API_WHSE_INBOUND_DELIVERY/",
      "destination": "ER9"
    },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "/test-resources/$1",
